<?xml version="1.0" encoding="utf-8"?>
<mesh:mesh xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:mesh="XMLMesh"
  xmlns:element="XMLMesh"
  xsi:schemaLocation="XMLMesh ../../xml_schemas/mesh_h2d_xml.xsd">
  <variables>
    <var name="a" value="1.0" />
    <var name="m_a" value="-1.0" />
    <var name="b" value="0.70710678118654757" />    
  </variables>

  <vertices>
  <!-- Contains all examples how it is possible to write a zero. -->
    <v x="-1.00000000000000000000" y="-1" i="0"/>
    <v x="1" y="-1" i="3"/>
    <v x="1" y="1" i="2"/>
    <v x="-1" y="1" i="1"/>
  </vertices>

  <elements>
    <mesh:q v1="0" v2="3" v3="2" v4="1" m="Aluminum" />
  </elements>

  <edges>
    <ed v1="0" v2="3" m="Bnd" />
    <ed v1="3" v2="2" m="Bnd" />
    <ed v1="2" v2="1" m="Bnd" />
    <ed v1="1" v2="0" m="Bnd" />
  </edges>
</mesh:mesh>