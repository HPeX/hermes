<?xml version="1.0" encoding="utf-8"?>
<mesh:mesh xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:mesh="XMLMesh"
  xmlns:element="XMLMesh"
  xsi:schemaLocation="XMLMesh ../../xml_schemas/mesh_h2d_xml.xsd">
  <variables>
    <var name="a" value="1.0" />
    <var name="2a" value="2.0" />
    <var name="b" value="1.0" />    
  </variables>

  <vertices>
  <!-- Contains all examples how it is possible to write a zero. -->
    <v x="0" y="0" i="0"/>
    <v x="a" y="0" i="1"/>
    <v x="a" y="b" i="2"/>
    <v x="0" y="b" i="3"/>
    <v x="2a" y="0" i="4"/>
    <v x="2a" y="b" i="5"/>
  </vertices>

  <elements>
    <mesh:q v1="0" v2="1" v3="2" v4="3" m="0" />
    <mesh:q v1="1" v2="4" v3="5" v4="2" m="0" />
  </elements>

  <edges>
    <ed v1="0" v2="1" m="0" />
    <ed v1="1" v2="4" m="1" />
    <ed v1="4" v2="5" m="2" />
    <ed v1="5" v2="2" m="3" />
    <ed v1="2" v2="3" m="4" />
    <ed v1="3" v2="0" m="5" />
  </edges>
</mesh:mesh>